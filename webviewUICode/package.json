{
  "name": "webview-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:websocket": "vite build --config websocket.vite.config.js",
    "buildbak": "npx esbuild ./src/worker/worker.ts --outfile=./src/worker/worker.js --format=iife --global-name=workerLib --bundle && vite build",
    "build:jscad": "npx esbuild ./node_modules/@jscad/modeling/src/index.js --outfile=./modeling.esm.js --minify --bundle --format=esm",
    "build:webview": "vite build --config webview.vite.config.js",
    "copy":"cp -r ./node_modules/@jscad/modeling ../myModule/",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "esbuild": "^0.25.8",
    "svelte": "^5.41.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.11"
  },
  "dependencies": {
    "@jscad/modeling": "^2.12.6",
    "@types/three": "^0.180.0",
    "@types/vscode": "^1.102.0",
    "@types/vscode-webview": "^1.57.5",
    "three": "^0.180.0"
  }
}
